<p id="notice"><%= notice %></p>

<h1>Question Set Junctions</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Question set</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_set_junctions.each do |question_set_junction| %>
      <tr>
        <td><%= question_set_junction.question %></td>
        <td><%= question_set_junction.question_set %></td>
        <td><%= link_to 'Show', question_set_junction %></td>
        <td><%= link_to 'Edit', edit_question_set_junction_path(question_set_junction) %></td>
        <td><%= link_to 'Destroy', question_set_junction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

