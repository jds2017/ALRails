<p id="notice"><%= notice %></p>

<h1>Course To Lecture Junctions</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Lecture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_to_lecture_junctions.each do |course_to_lecture_junction| %>
      <tr>
        <td><%= course_to_lecture_junction.course %></td>
        <td><%= course_to_lecture_junction.lecture %></td>
        <td><%= link_to 'Show', course_to_lecture_junction %></td>
        <td><%= link_to 'Edit', edit_course_to_lecture_junction_path(course_to_lecture_junction) %></td>
        <td><%= link_to 'Destroy', course_to_lecture_junction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course To Lecture Junction', new_course_to_lecture_junction_path %>
